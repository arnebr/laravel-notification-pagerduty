{
    "name": "laravel-notification-channels/pagerduty",
    "description": "A Laravel notification channel for sending PagerDuty events.",
    "keywords": [
        "laravel",
        "laravel-package",
        "notifications",
        "notification-channel",
        "pagerduty",
        "alerting"
    ],
    "homepage": "https://github.com/laravel-notification-channels/pagerduty",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Waite",
            "email": "lwaite@gmail.com",
            "homepage": "https://lukewaite.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/notifications": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^7.0 || ^8.0 || ^9.0 || ^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5|^9.5|^10.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PagerDuty\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\PagerDuty\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
